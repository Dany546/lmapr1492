    "\n",
    "\n",
    "mpr = MPRester(\"qCZVQk878CsirPAezSTx\") \n",
    "\n",
    "data = mpr.query(\"mp-23040\", [\"cif\"])[0]\n",
    "\n",
    "data2 = open(\"Ca2PI_mp-23040.cif\", \"w\")\n",
    "\n",
    "\n",
    "for key, value in data.items():\n",
    "    \n",
    "    data2.write(str(value))\n",
    "    \n",
    "    \n",
    "data2.close()    "
